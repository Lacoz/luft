in:
   type: {{ env.TYPE }}
   driver_path: {{ env.DRIVER_PATH }}
   host: {{ env.URI }}
   port: {{ env.PORT }}
   user: {{ env.USER }}
   password: {{ env.PASSWORD }}
   database: {{ env.DATABASE }}
   connect_timeout: 1800
   {{ env.FETCH_ROWS }}
   {{ env.SCHEMA }}
   table: {{ env.SOURCE_TABLE }}
   default_timezone: CET
   options: {useTimezone: true, serverTimezone: Europe/Prague}
   select: "{{ env.COLUMNS }}"
   where: {{ env.WHERE_CLAUSE }}
   {{ env.COLUMN_OPTIONS }}
{{ env.FILTERS}}